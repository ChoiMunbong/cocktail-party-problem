{
    "data_preprocessing": {
        "general": {
            "n_src": 2,
            "balance": false,
            "n_individuals": null,
            "n_open": 3,
            "zipfile_save": false
        },
        "classifier": {
            "augmentation_factor": 4,
            "shift_factor": 0.1,
            "padding_scheme": "zero",
            "padding_side": "front"
        },
        "separator": {
            "mixing_to_memory": {
                "n_src": 2,
                "training_size": 12000,
                "validation_size": 3000,
                "shift_factor": 0.1,
                "shift_overlaps": false,
                "padding_scheme": "zero",
                "padding_side": "front"
            }
        }
    },
    "classifier_dataset_params": {
        "n_src": 2,
        "objective": "classification",
        "stft_params": null,
        "filter_params": null,
        "shuffle": true
    },
    "classifier_learning_params": {
        "batch_size": 32,
        "learning_rate": 0.0001,
        "epochs": 100
    },
    "classifier_model_params": {
        "in_size": [
            null,
            1,
            11025
        ],
        "n_classes": 11,
        "n_blocks": 3,
        "pool_size": 1,
        "input_mode": "raw",
        "stft_params": {
            "kernel_size": 1024,
            "stride": 512
        },
        "lin_dim": 128,
        "dropout": 0.25
