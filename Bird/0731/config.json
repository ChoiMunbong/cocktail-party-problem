    "classifier_trainer_params": {
        "loss_func": {
            "nll_loss": "nll_weighted"
        },
        "metric_func": {
            "accuracy": "classifier_acc"
        },
        "verbose": 1,
        "device": "cuda",
        "dest": "Classifier",
        "model_saver_callback": {
            "epoch": 30,
            "save_every": 1
        }
    },
    "separator_dataset_params": {
        "n_src": 2,
        "objective": "separation",
        "stft_params": null,
        "filter_params": null
