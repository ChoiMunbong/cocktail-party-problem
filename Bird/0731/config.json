    "separator_learning_params": {
        "batch_size": 32,
        "learning_rate": 0.0001,
        "epochs": 100,
        "optimizer": "adamw",
        "momentum": 0.6
    },
    "separator_model_params": {
        "in_size": [
            null,
            1,
            11025
        ],
        "n_src": 2,
        "n_blocks": 3,
        "pool_size": 1,
        "batch_norm": true,
        "filterbank_params": {
            "nfft": 1024,
